{
  "name": "frontend_shop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev-local-backend": "API_ENDPOINT=http://localhost:8000 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "init-locales": " mkdir -p locales && (test -f locales/en.json || echo {} > locales/en.json) && (test -f locales/ru.json || echo {} > locales/ru.json)",
    "parse-locales": "node parse-locales -p \"locales\" -r report -m ru -l ru -l en",
    "report-influencer": "vue-i18n-extract report -v \"../influencer/**/*.vue\" -o locales/report.json -l locales/*.json",
    "report-common": "vue-i18n-extract report -v \"../common/**/*.vue\" -o locales/report.json -l locales/*.json",
    "make-locales-influencer": "npm run report-influencer && npm run parse-locales",
    "make-locales-common": "npm run report-common && npm run parse-locales",
    "make-locales": "npm run make-locales-influencer && npm run make-locales-common"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/sentry": "^5.1.5",
    "@panter/vue-i18next": "^0.15.2",
    "card-validator": "^8.1.1",
    "cookieparser": "^0.1.0",
    "core-js": "^3.19.1",
    "dadata-suggestions": "^1.1.1",
    "i18next": "^17.0.6",
    "js-cookie": "^3.0.1",
    "libphonenumber-js": "^1.9.43",
    "nuxt": "^2.15.8",
    "nuxt-env": "^0.1.0",
    "nuxt-vuex-localstorage": "^1.3.0",
    "qs": "^6.10.1",
    "sass": "^1.49.0",
    "swiper": "^6.8.4",
    "v-mask": "^2.3.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-click-outside": "^1.1.0",
    "vue-dadata": "^1.4.12",
    "vue-i18n": "^8.25.0",
    "vue-i18n-extract": "^1.0.2",
    "vue-rx": "^6.2.0",
    "vue-yandex-maps": "^0.11.1",
    "vuedraggable": "^2.24.3",
    "vuejs-clipper": "^3.1.2",
    "vuelidate": "^0.7.6",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "axios": "^0.24.0",
    "babel-plugin-i18next-extract": "^0.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "vue-style-loader": "^4.1.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "4.46.0"
  }
}
